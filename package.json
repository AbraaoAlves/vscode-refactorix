{
	"name": "vscode-refactorix",
	"description": "TypeScript refactoring tools for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "christian@o3tt3rli.com",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:typescript"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [{
			"command": "extension.refactorixExtractVariable",
			"title": "Extract variable"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}